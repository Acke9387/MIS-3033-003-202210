<Window x:Class="JSON_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnGo" Content="Button" HorizontalAlignment="Left" Margin="416,95,0,0" VerticalAlignment="Top" Click="btnGo_Click"/>
        <ListBox x:Name="lstPeople" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,188,0,0"/>

    </Grid>
</Window>
